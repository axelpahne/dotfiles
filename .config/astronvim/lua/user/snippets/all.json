{
  "Print filename": {
    "prefix": "ff",
    "body": ["${TM_FILENAME_BASE}"]
  },
  "Placeholder": {
    "prefix": "placeholder",
    "body": ["//via.placeholder.com//${1:1600}x${2:900}"]
  },
  "Placekitten": {
    "prefix": "placekitten",
    "body": ["//placekitten.com//${1:1600}/${2:900}"]
  },
  "Unsplash": {
    "prefix": "unsplash",
    "body": ["//source.unsplash.com/${1:1600}x${2:900}"]
  },
  "Print to console": {
    "prefix": "log",
    "body": ["console.log($1)"]
  },
  "Import react": {
    "prefix": "irea",
    "body": "import * as React from 'react'"
  },
  "Import propTypes": {
    "prefix": "ipro",
    "body": ["import PropTypes from 'prop-types'"]
  },
  "Import mui": {
    "prefix": "imui",
    "body": "import { $2 } from '@mui/$1'"
  },
  "Import noa": {
    "prefix": "inoa",
    "body": "import { $2 } from '@noaignite/$1'"
  },
  "Import lingui": {
    "prefix": "ilin",
    "body": "import { t } from '@lingui/macro'"
  },
  "Export default": {
    "prefix": "ee",
    "body": ["export { default } from './${1:Component}'"]
  },
  "Export default as": {
    "prefix": "eee",
    "body": ["export { default as ${1:Component} } from './${1:Component}'"]
  },
  "Import module": {
    "prefix": "ii",
    "body": ["import $2 from '$1'"]
  },
  "Import local module": {
    "prefix": "iii",
    "body": ["import $2 from '~/${1:components}'"]
  },
  "Translate w/ literas": {
    "prefix": "tt",
    "body": ["t(${2:__translationGroup})`$1`"]
  },
  "Mui styled w/o theme": {
    "prefix": "ss",
    "body": [
      "const ${1:${TM_FILENAME_BASE}}${2:Root} = styled(${3:'div'})({",
      "  ${4}",
      "})"
    ]
  },
  "Mui styled w/ theme": {
    "prefix": "sss",
    "body": [
      "const ${1:${TM_FILENAME_BASE}}${2:Root} = styled(${3:'div'})(({ theme }) => ({",
      "  ${4}",
      "}))"
    ]
  },
  "React danger": {
    "prefix": "danger",
    "body": ["dangerouslySetInnerHTML={{ __html: $1 }}"]
  },
  "React useCallback": {
    "prefix": "ruc",
    "body": ["const $1 = React.useCallback(($2) => {", "  $4", "}, [$3])"]
  },
  "React useEffect": {
    "prefix": "rue",
    "body": ["React.useEffect(() => {", "  $2", "}, [$1])"]
  },
  "React useMemo": {
    "prefix": "rum",
    "body": ["const $1 = React.useMemo(", "  () => ({ $2 }),", "  [$2]", ")"]
  },
  "React useRef": {
    "prefix": "rur",
    "body": ["const $1Ref = React.useRef(${2:null})"]
  },
  "React useState": {
    "prefix": "rus",
    "body": ["const [$1, set${1/(.*)/${1:/capitalize}/}] = React.useState($2)"]
  },
  "Storybook Component Story Format 2": {
    "prefix": "csf2",
    "body": [
      "import * as React from 'react'",
      "import ${1:MyComponent} from './${1:MyComponent}'",
      "",
      "export default {",
      "  title: '${2:Components}/${1:MyComponent}',",
      "  component: ${1:MyComponent},",
      "}",
      "",
      "const Template = (args) => <${1:MyComponent} {...args} />",
      "",
      "export const Default = Template.bind({})",
      "Default.args = {",
      "  children: 'Just some children',",
      "}"
    ]
  },
  "Storybook Component Story Format 3": {
    "prefix": "csf3",
    "body": [
      "import type { Meta, StoryObj } from '@storybook/react'",
      "import ${1:MyComponent} from './${1:MyComponent}'",
      "",
      "const meta: Meta<typeof ${1:MyComponent}> = {",
      "  component: ${1:MyComponent},",
      "}",
      "",
      "export default meta",
      "type Story = StoryObj<typeof ${1:MyComponent}>",
      "",
      "export const Default: Story = {",
      "  args: {",
      "    children: 'Just some children',",
      "  },",
      "}",
      ""
    ]
  },
  "React function": {
    "prefix": "rf",
    "body": [
      "import * as React from 'react'",
      "",
      "function ${1:${TM_FILENAME_BASE}}(props) {",
      "  return (",
      "    <div>",
      "      $2",
      "    </div>",
      "  )",
      "}",
      "",
      "export default ${1:${TM_FILENAME_BASE}}"
    ]
  },
  "React function forwardRef": {
    "prefix": "rff",
    "body": [
      "import * as React from 'react'",
      "import { OverridableComponent } from '@mui/types'",
      "import { ${1:${TM_FILENAME_BASE}}Props, ${1:${TM_FILENAME_BASE}}TypeMap } from './${1:${TM_FILENAME_BASE}}Props'",
      "",
      "const ${1:${TM_FILENAME_BASE}} = React.${2:forwardRef}(function ${1:${TM_FILENAME_BASE}}(props: ${1:${TM_FILENAME_BASE}}Props, ref) {",
      "  const { children, component: ComponentProp = 'div', ...other } = props",
      "",
      "  return (",
      "    <ComponentProp ref={ref} {...other}>",
      "      {children}",
      "    </ComponentProp>",
      "  )",
      "}) as OverridableComponent<${1:${TM_FILENAME_BASE}}TypeMap>",
      "",
      "export default ${1:${TM_FILENAME_BASE}}"
    ]
  },
  "React function type map & props": {
    "prefix": "typemap",
    "body": [
      "import * as React from 'react'",
      "import { OverrideProps } from '@mui/types'",
      "",
      "export interface ${1:${TM_FILENAME_BASE}}TypeMap<",
      "  AdditionalProps = {},",
      "  DefaultComponent extends React.ElementType = '${2:div}',",
      "> {",
      "  props: AdditionalProps & {",
      "    $3",
      "  }",
      "  defaultComponent: DefaultComponent",
      "}",
      "",
      "export type ${1:${TM_FILENAME_BASE}}Props<",
      "  RootComponent extends React.ElementType = ${1:${TM_FILENAME_BASE}}TypeMap['defaultComponent'],",
      "  AdditionalProps = {},",
      "> = OverrideProps<${1:${TM_FILENAME_BASE}}TypeMap<AdditionalProps, RootComponent>, RootComponent> & {",
      "  component?: React.ElementType",
      "}"
    ]
  },
  "Sanity define type": {
    "prefix": "dt",
    "body": [
      "defineType({",
      "  title: '${1/(.*)/${1:/capitalize}/}',",
      "  name: '$1',",
      "  type: 'object',",
      "  fields: [",
      "    $2",
      "  ],",
      "})"
    ]
  },
  "Sanity define field": {
    "prefix": "df",
    "body": [
      "defineField({",
      "  title: '${1/(.*)/${1:/capitalize}/}',",
      "  name: '$1',",
      "  type: '$2',$3",
      "}),$4"
    ]
  }
}
